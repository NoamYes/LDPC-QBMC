function [out_vec] = iter(H,vec)
%UNTITLED Summary of this function goes here
%   Detailed explanation goes here
out_vec = vec;
questionMark = 2; % we can change this later
change_vec = find(~(H*vec.'-questionMark)); %find the locations of 'lonely' question marks
for i = change_vec
    tmp = find(~(H(i,:).*vec-questionMark)); %find the location of the question mark in the original vector
    out_vec(tmp) = 0; %cheating here, by updating it to be 0 and not XOR of all the rest (since
end
end

